<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="core.Email" table="temail" catalog="solocate">
		<id name="id" column="id" type="long" unsaved-value="999999">
			<generator class="increment"/>
		</id>
		<property name="email" column="email" type="string"/>
		<property name="description" column="description" type="int"/>
		<property name="contact" column="contact" type="long"/>
	</class>
</hibernate-mapping>